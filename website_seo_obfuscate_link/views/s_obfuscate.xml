<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="s_obfuscate_link" name="Obfuscate Link">
        <div>
            <button type="button" class="seo_obfuscated_link" onclick="javascript:location.href='/page/website.contactus';">
                Contact us
            </button>
        </div>
    </template>

    <template id="s_obfuscate_place_in_tab" inherit_id="website.snippets">
    <xpath expr="//div[@id='snippet_content']/div[@class='o_panel_body']" position="inside">
        <t t-snippet="website_seo_obfuscate_link.s_obfuscate_link"
        t-thumbnail="/website_seo_obfuscate_link/static/src/img/s_obfuscate.png"/>
    </xpath>
    </template>


</odoo>
